<view>
  <map 
    id="map" 
    longitude="{{longitude}}" 
    latitude="{{latitude}}" 
    scale="15.2" 
    controls="{{controls}}"
    bindcontroltap="controltap" 
    bindcallouttap="markertap" 
    markers="{{markers}}" 
    bindmarkertap="markertap"
    polyline="{{polyline}}" 
    polygons="{{polygons}}" 
    bindregionchange="regionchange" 
    show-location 
    style="width: 100%; height: 100vh;" >
    <view>

<button bindtap="test">start</button>
    </view>

  </map>

</view>